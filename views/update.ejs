        <h2 class="u-text u-text-1">Update Employees</h2>
        <div class="u-form u-form-1">
          <form action="/editemp/<%=staff.id%>" method="POST"  name="form">
            <div class="u-form-group u-form-name">
              <label for="name-3b9a" class="u-label u-label-1">First Name</label>
              <input type="text" placeholder="Enter your First Name" value="<%=staff.fname%>" id="name-3b9a" name="fname" class="u-border-1 u-border-grey-30 u-input u-input-rectangle u-white" required="">
            </div>
            <div class="u-form-group u-form-name u-form-group-2">
              <label for="name-bd92" class="u-label u-label-2">Last Name</label>
              <input type="text" placeholder="Enter your Last Name" value="<%=staff.lname%>" id="name-bd92" name="lname" class="u-border-1 u-border-grey-30 u-input u-input-rectangle u-white" required="">
            </div>
            <div class="u-form-email u-form-group">
              <label for="email-3b9a" class="u-label u-label-3">Email-Address</label>
              <input type="email" placeholder="Enter a valid email address" value="<%=staff.email%>" id="email-3b9a" name="email" class="u-border-1 u-border-grey-30 u-input u-input-rectangle u-white" required="">
            </div>
            <div class="u-form-group u-form-phone u-form-group-4">
              <label for="phone-5567" class="u-label u-label-4">Mobile No.</label>
              <input type="text"  placeholder="Enter your phone (e.g +917525044400)" value="<%=staff.phone%>" id="phone-5567" name="phone" class="u-border-1 u-border-grey-30 u-input u-input-rectangle u-white" required="">
            </div>
            <div class="u-form-date u-form-group u-form-group-5">
              <label for="date-daa1" class="u-label u-label-5">DOB</label>
              <input type="date" placeholder="mm/dd/yyyy" id="date-daa1" value="<%=staff.date%>" name="date" class="u-border-1 u-border-grey-30 u-input u-input-rectangle u-white" required="">
            </div>
            <div class="u-form-group u-form-radiobutton u-form-group-6">
              <div class="u-form-radio-button-wrapper">
                <input type="radio" name="gender" value="Male" required="required">
                <label class="u-label u-label-6" for="radiobutton">Male</label>
                <br>
                <input type="radio" name="gender" value="Female" required="required">
                <label class="u-label u-label-7" for="radiobutton">Female</label>
                <br>
              </div>
            </div>
            <div class="u-form-group u-form-group-7">
              <label for="text-7199" class="u-label u-label-8">Designation</label>
              <input type="text" placeholder="Enter designation" id="text-7199" value="<%=staff.designation%>" name="designation" class="u-border-1 u-border-grey-30 u-input u-input-rectangle u-white">
            </div>
            <div class="u-align-left u-form-group u-form-submit">
              <button type="submit" class="u-btn u-btn-submit u-button-style">Update</button>
             
            </div>
           
          </form>
        </div>
